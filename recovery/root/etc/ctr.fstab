# mount point   fstype    device        [device2]    fs_options    [fs_options2]    lun

/boot		emmc	/dev/block/bootdevice/by-name/boot
/system		emmc	/dev/block/bootdevice/by-name/recovery
/system		ext4	/dev/block/bootdevice/by-name/system
/cache		ext4	/dev/block/bootdevice/by-name/cache
/data		ext4	/dev/block/bootdevice/by-name/userdata 	 length=-16384
/sdcard		datamedia   /dev/null
/external_sd	vfat    /dev/block/mmcblk1p1    /dev/block/mmcblk1	lun=sys/class/android_usb/android0/f_mass_storage/lun/file
/usb-otg	auto	/dev/block/sda1 /dev/block/sda

